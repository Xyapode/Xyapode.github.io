{% extends "base.html.twig" %}

{% block body %}

    <div class="fo">
        <form action="/tableau-de-bord">
            <label for="fname">Prénom</label>
            <input type="text" id="fname" name="firstname" placeholder="Votre prénom...">

            <label for="lname">Nom de famille</label>
            <input type="text" id="lname" name="lastname" placeholder="Votre nom de famille...">

            <label for="email">Email</label>
            <input type="email" id="lname" name="lastname" placeholder="Votre email...">

            <label for="password">Mot de passe</label>
            <input type="password" id="lname" name="lastname" placeholder="Votre mot de passe...">

            <input type="submit" value="S'inscrire">
        </form>
    </div>

{% endblock %}